{
  "private": true,
  "homepage": "http://postcss.com/",
  "scripts": {
    "start": "babel-node scripts/build --server --open --dev",
    "static": "NODE_ENV=production babel-node scripts/build --static --production",
    "test": "npm run static",
    "predeploy": "npm run static",
    "deploy": "./scripts/deploy.sh",
    "_ci-deploy": "GIT_DEPLOY_REPO=https://$GH_TOKEN@github.com/postcss/postcss.com.git ./scripts/deploy.sh",
    "ci-deploy": "if [ \"$TRAVIS_PULL_REQUEST\" = \"false\" ] && [ \"$TRAVIS_BRANCH\" = \"master\" ]; then npm run _ci-deploy; fi;",
    "test": "npm run ci-deploy",
    "postbuild": "echo 'postcss.com' > dist/CNAME && touch dist/.nojekyll"
  },
  "babel": {
    "stage": 0
  },
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "babel": "^5.8.34",
    "babel-core": "^5.8.34",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^5.4.0",
    "babel-plugin-react-transform": "^1.1.1",
    "css-loader": "^0.17.0",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.14.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.5",
    "highlight.js": "^8.9.1",
    "history": "^1.17.0",
    "invariant": "^2.2.0",
    "json-loader": "^0.5.4",
    "markdown-it": "^5.0.3",
    "markdown-it-toc-and-anchor": "^1.0.1",
    "postcss-custom-media": "^5.0.0",
    "postcss-custom-properties": "^5.0.0",
    "postcss-loader": "^0.7.0",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-helmet": "^2.2.0",
    "react-redux": "^2.1.2",
    "react-router": "^1.0.3",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "redux": "^2.0.0",
    "redux-devtools": "^2.1.5",
    "redux-thunk": "^0.1.0",
    "statinamic": "^0.5.1",
    "style-loader": "^0.12.4",
    "webpack": "^1.12.9",
    "whatwg-fetch": "^0.9.0"
  }
}
